[flake8]
ignore = B305
exclude =
    .git,
    __pycache__,
    .venv,
    env,
    tests,
    settings/env,
    local_test.py
max-complexity = 10
max-line-length = 120
import-order-style = pep8
application-import-names = app, utils, settings, src
format = '%(path)s:%(row)d Column:%(col)d: %(code)s %(text)s'
